{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "biome.requireConfiguration": true,
  "editor.fontFamily": "Monaco, 'Courier New', monospace, 'Apple Color Emoji'",
  "biome.requireConfigFile": true,
  "sshfs.configs": [
    {
      "host": "192.168.10.115",
      "name": "RaspberryPi",
      "password": "9mltmfny",
      "port": 8080,
      "privateKeyPath": "/Users/shilly/.ssh/id_rsa",
      "root": "/",
      "username": "pi"
    }
  ],
  "git.autofetch": true,
  "editor.tabSize": 2,
  "files.associations": {
    "*.erb": "erb",
    "*.mdc": "markdown"
  },
  "emmet.includeLanguages": {
    "html.erb": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "key": "cmd+shift+`",
  "command": "erb.toggleTags",
  "when": "editorTextFocus && editorLangId == erb",
  "[html]": {
    "editor.insertSpaces": true,
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": [
      "document"
    ],
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": [
      "document"
    ]
  },
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnSave": true
  },
  "rubyLsp.rubyVersionManager": {
    "identifier": "mise",
    "miseExecutablePath": "/opt/homebrew/bin/mise"
  },
  "rubyLsp.formatter": "auto",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "svipas.prettier-plus"
  },
  "[sql]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "files.autoSave": "off",
  "files.eol": "\n",
  "files.autoGuessEncoding": true,
  "beautify.language": {
    "js": {
      "type": [
        "javascript",
        "json"
      ],
      "filename": [
        ".jshintrc",
        ".jsbeautifyrc",
        "json"
      ]
    },
    "css": [
      "css",
      "scss"
    ],
    "html": [
      "htm",
      "html",
      ".html.erb"
    ]
  },
  "hediet.vscode-drawio.local-storage": "",
  "todo.colors.comment": "#00ffff",
  "indentRainbow.ignoreErrorLanguages": [
    "ruby",
    "markdown",
    "erb"
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "git.suggestSmartCommit": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "todo-tree.highlights.highlightDelay": 100,
  "prettier.embeddedLanguageFormatting": "off",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": false,
  "dart.runPubGetOnPubspecChanges": false,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.iconTheme": "vscode-icons",
  "json": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.formatOnSave": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "dart.flutterSdkPaths": [
    "/Users/shilly/.local/share/mise/installs/flutter/3.24.3-stable/bin/flutter"
  ],
  "settingsSync.ignoredSettings": [
